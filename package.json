{
  "name": "lazarus-ui",
  "theme": "ui",
  "version": "0.1.1",
  "description": "Atom Lazarus UI theme",
  "keywords": [
    "ui",
    "theme",
    "dark"
  ],
  "repository": "https://github.com/delta-one/lazarus-ui",
  "homepage": "https://github.com/delta-one/lazarus-ui",
  "bugs": {
    "url": "https://github.com/delta-one/lazarus-ui/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0"
  },
  "author": {
    "name": "Patrick Franz"
  },
  "devDependencies": {
    "coffeelint": "^1.9.7"
  },
  "configSchema": {
    "fontSize": {
      "title": "Font Size",
      "description": "Change the UI font size. Needs to be between 8 and 20.",
      "type": [
        "integer",
        "string"
      ],
      "minimum": 8,
      "maximum": 20,
      "default": "Auto"
    }
  }
}
